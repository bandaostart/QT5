import QtQuick 1.1


//Gradient：项目中使用Gradient项目来定义一个渐变
Rectangle{
    width: 100; height: 100
    gradient: Gradient{
        GradientStop{ position: 0.0;  color:  "red"}
        GradientStop{ position: 0.33; color:  "yellow"}
        GradientStop{ position: 1.0;  color: "green"}
    }

}





























































































































